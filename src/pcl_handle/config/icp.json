{
    "//": "处理管线设置",
    "pcl_pipe_line": [
        [
            "PclStage",
            "PclVoxel",
            "//PclFeaturesExtract",
            "//PclFPFHIcp",
            "//PclNdt",
            "//PclIcp",
            "//PclTransform",
            "//JsonPrint",
            "PclPublish",
            "PclSave"
        ]
    ],
    "PclPop": {
        "default": {
            "input": {
                "pointcloud_name": "global_map"
            }
        }
    },
    "PclIcp": {
        "default": {
            "yaw_offset": 30.0,
            "yaw_resolution": 10.0,
            "xy_offset": 0.2,
            "random_icp": false,
            "max_iterations": 10,
            "transformation_epsilon": 0.1,
            "max_correspondence_distance": 1.0,
            "euclidean_fitness_epsilon": 0.1,
            "input": {
                "pointcloud_name": "global_map",
                "transform_name": "icp_transform"
            },
            "output": {
                "pointcloud_name": "dd",
                "transform_name": "icp_transform_out"
            }
        }
    },
    "PclFeaturesExtract": {
        "default": {
            "radius": 0.5,
            "extractfphf": true,
            "fphfkSearch": 10,
            "output": {
                "normal_name": "pointcloud_normal",
                "fphf_name": "pointcloud_fpfh"
            }
        }
    },
    "PclVoxel": {
        "default": {
            "voxel_size": 0.5
        }
    },
    "PclFPFHIcp": {
        "default": {
            "input": {
                "map_pointcloud_name": "map_publish",
                "transform_name": "icp_transform",
                "fpfh_source": "pointcloud_fpfh",
                "fpfh_map": "map_fpfh"
            },
            "output": {
                "transform_name": "icp_transform_out"
            }
        }
    },
    "PclNdt": {
        "default": {
            "resolution": 1.0,
            "iterations": 40,
            "step_size": 0.1,
            "epsilon": 0.001,
            "input": {
                "map_pointcloud_name": "map_publish",
                "transform_name": "icp_transform"
            },
            "output": {
                "transform_name": "icp_transform_out"
            }
        }
    },
    "PclTransform": {
        "default": {
            "input": {
                "transform_name": "icp_transform_out"
            }
        }
    },
    "PclPublish": {
        "default": {
            "settings": [
                {
                    "name": "map_publish",
                    "topic_name": "/pcl_map",
                    "target_transform": "laser_link"
                },
                {
                    "name": "PclRaw",
                    "topic_name": "/pcl_raw",
                    "target_transform": "laser_link"
                }
            ]
        }
    },
    "PclStage": {
        "default": {
            "output": {
                "pointcloud_name": "PclRaw"
            },
            "copy": true
        }
    }
}