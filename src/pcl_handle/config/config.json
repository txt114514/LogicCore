{
    "//": "处理管线设置",
    "pcl_pipe_line": [
        [
            "PclStage",
            "PclFilterZero",
            "PclSegmentation",
            "PclCenterTransform",
            "JsonPrint",
            "PclPublish"
        ]
    ],
    "//": "min_z ,max_z为z轴过滤,zero_z表示当前零点位置,max_r为距离中心点过滤,r为0不生效",
    "PclFilterZero": {
        "default": {
            "min_z": 0.5,
            "max_z": 3.2,
            "max_r": 6,
            "zero_z": 0.181
        },
        "id1": {
            "min_z": 2.3,
            "max_z": 2.9,
            "zero_z": 0.0,
            "max_r": 0
        }
    },
    "//": "distance_threshold为分割阈值",
    "PclSegmentation": {
        "default": {
            "distance_threshold": 0.1,
            "output": {
                "plane_name": "plane_coefficients"
            }
        }
    },
    "PclStage": {
        "default": {
            "output": {
                "pointcloud_name": "PclRaw"
            },
            "copy": true
        }
    },
    "PclPublish": {
        "default": {
            "settings": [
                {
                    "name": "PclRaw",
                    "topic_name": "/pcl_raw",
                    "target_transform": ""
                },
                {
                    "name": "center_points",
                    "topic_name": "/center_points",
                    "target_transform": ""
                }
            ]
        }
    },
    "//": "point_size为点大小,point_color为点颜色,background_color为背景颜色,show_axes为是否显示坐标轴,show_normals为是否显示法线",
    "PclVisual": {
        "default": {
            "point_size": 1,
            "point_color": [
                0,
                255,
                0
            ],
            "background_color": [
                0,
                0,
                0
            ],
            "show_axes": true,
            "show_normals": false
        }
    },
    "PclCenterTransform": {
        "default": {
            "inputs": {
                "plane_name": "plane_coefficients"
            },
            "output": {
                "center_points_name": "center_points",
                "transform_name": "transform"
            },
            "print": true
        }
    },
    "//": "default_color为默认颜色,addition_settings为额外颜色设置,前三个是rgb最后是点大小",
    "PclMultiVisual": {
        "default": {
            "default_color": [
                0,
                255,
                0
            ],
            "addition_settings": [
                {
                    "name": "center_points",
                    "color": [
                        255,
                        0,
                        255
                    ],
                    "size": 5
                }
            ],
            "background_color": [
                0,
                0,
                0
            ],
            "show_axes": true,
            "show_normals": false
        }
    }
}