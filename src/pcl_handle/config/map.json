{
    "//": "处理管线设置",
    "map_pipe_line": [
        [
            "PcdRead",
            "PclVoxel",
            "PclFeaturesExtract",
            "PclStage",
            "JsonPrint"
        ]
    ],
    "PclPop": {
        "default": {
            "input": {
                "pointcloud_name": "global_map"
            }
        }
    },
    "PcdRead": {
        "default": {
            "pcd": "/../../scans.pcd",
            "output": {
                "pointcloud_name": "global_map"
            }
        }
    },
    "PclFeaturesExtract": {
        "default": {
            "radius": 1.5,
            "extractfphf": true,
            "fphfkSearch": 10,
            "output": {
                "normal_name": "map_normal",
                "fphf_name": "map_fpfh"
            }
        }
    },
    "PclVoxel": {
        "default": {
            "voxel_size": 0.8
        }
    },
    "PclPublish": {
        "default": {
            "settings": [
                {
                    "name": "map_publish",
                    "topic_name": "/pcl_map",
                    "target_transform": "map"
                }
            ]
        }
    },
    "PclVisual": {
        "default": {
            "point_size": 1,
            "point_color": [
                0,
                255,
                0
            ],
            "background_color": [
                0,
                0,
                0
            ],
            "show_axes": true,
            "show_normals": true,
            "input": {
                "normal_name": "map_normal"
            }
        }
    },
    "PclStage": {
        "default": {
            "output": {
                "pointcloud_name": "map_publish"
            },
            "copy": true
        }
    }
}